{
  "name": "@vigilio/express",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "create web apps with @vigilio/express now",
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "serve": "bun --watch app/main.ts ",
    "build:dev": "astro check  && astro build ",
    "start": "NODE_ENV=production bun dist/main.js",
    "test": "vitest",
    "biome:format:server": " biome format  ./app --write",
    "biome:check:server": "biome check ./app",
    "biome:format:client": "biome format  ./resources --write",
    "biome:check:client": "biome check ./resources",
    "biome:format": "bun biome:format:server && bun biome:format:client",
    "biome:check": "bun biome:check:server && bun biome:check:client"
  },
  "keywords": [
    "@vigilio/express",
    "@vigilio/express-api",
    "bun"
  ],
  "author": "Vigilio",
  "license": "ISC",
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/node": "^8.2.5",
    "@astrojs/preact": "^3.3.0",
    "@astrojs/tailwind": "^5.1.0",
    "@decorators/di": "^2.0.0",
    "@decorators/express": "^2.9.4",
    "@hookform/resolvers": "^3.3.3",
    "@preact/signals": "^1.2.3",
    "@vigilio/express-core": "1.8.3",
    "@vigilio/preact-fetching": "^2.0.11",
    "@vigilio/preact-paginator": "^0.0.8",
    "@vigilio/preact-table": "^1.0.16",
    "@vigilio/sweet": "^1.1.0",
    "aos": "^2.3.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "^3.5.1",
    "js-cookie": "^3.0.5",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "preact": "^10.22.0",
    "react-hook-form": "^7.51.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sharp": "0.33.3",
