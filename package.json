    "@types/formidable": "^3.4.5",
    "@types/js-cookie": "^3.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.3.2",
    "@types/react-slick": "^0.23.13",
